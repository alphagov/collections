<% guidance ||= nil %>

<div class="govuk-body">
  <% if guidance.present? && guidance[:links].present? %>
      <ul class="govuk-list">
      <% guidance[:links].each do |link| %>
        <li><%= link_to(link[:label], link[:url], class: "govuk-link") %></li>
      <% end %>
      </ul>
  <% end %>
</div>
